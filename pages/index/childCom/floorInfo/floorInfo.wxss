/* pages/index/childCom/floorInfo/floorInfo.wxss */
.floor_title {
  margin: 20rpx 0;
  width: 522rpx;
  height: 63rpx;
}
.floor_title image {
  width: 100%;
}
.floor_list {
  overflow: hidden;
}
.floor_list navigator {
  float: left;
  width: 33.33%;
}
.floor_list navigator:nth-last-child(-n+4) image {
  /* 原图的宽高 232 *386 */
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-child(2),
.floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_list navigator:nth-child(1) {
  margin-top: 5rpx;
}
.floor_list navigator image {
  width: 100%;
  height: 100%;
}
